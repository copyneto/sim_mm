<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:table="sap.ui.table" xmlns="sap.m" xmlns:fb="sap.ui.comp.filterbar" xmlns:vh="sap.ui.comp.valuehelpdialog" xmlns:f="sap.ui.layout.form" xmlns:mvc="sap.ui.core.mvc">                                   
    <table:Column id="_IDGenColumn4" width="15rem">                                                                                                                                                                                                            
        <Label id="_IDGenLabel4" text="Descarga"/>                                                                                                                                                                                                             
        <table:template>                                                                                                                                                                                                                                       
            <StepInput id="Descarga" value="{descarga}" min="0" displayValuePrecision="3" width="120px" step="1.000" largerStep="10.000" description="{Unit}" change="onDescarga"/>                                                                            
        </table:template>                                                                                                                                                                                                                                      
        <table:customData>                                                                                                                                                                                                                                     
            <core:CustomData key="p13nData" value="\{&quot;columnKey&quot;: &quot;descarga&quot;, &quot;columnIndex&quot;: &quot;1&quot;, &quot;leadingProperty&quot;: &quot;descarga&quot;}"/>                                                                
        </table:customData>                                                                                                                                                                                                                                    
    </table:Column>                                                                                                                                                                                                                                            
    <!-- Coluna UF destino-->                                                                                                                                                                                                                                  
    <table:Column id="_IDGenColumn2" width="10rem">                                                                                                                                                                                                            
        <Label id="_IDGenLabel" text="Motivo"/>                                                                                                                                                                                                                
        <table:template>                                                                                                                                                                                                                                       
            <Input id="motivomov"&nbsp;                                                                                                                                                                                                                        
                valueHelpRequest=".valueHelpMotivo"&nbsp;                                                                                                                                                                                                      
                showSuggestion="true"&nbsp;                                                                                                                                                                                                                    
                showValueHelp="true"&nbsp;                                                                                                                                                                                                                     
                valueStateText="Motivo não existe"&nbsp;                                                                                                                                                                                                       
                value="{path : 'motivomov'}"&nbsp;                                                                                                                                                                                                             
                liveChange=".onliveChangeMotivo"                                                                                                                                                                                                               
                submit=".onSubmitMotivo"&nbsp;                                                                                                                                                                                                                 
                maxLength="2"                                                                                                                                                                                                                                  
                suggestionItems="{                                                                                                                                                                                                                             
				path: '/ZI_MM_VH_GRUND',                                                                                                                                                                                                                                   
				length: 10                                                                                                                                                                                                                                                 
			}">                                                                                                                                                                                                                                                         
                <suggestionItems>                                                                                                                                                                                                                              
                    <core:ListItem id="_IDGenListItem3" key="{motivomov}" text="{                                                                                                                                                                              
						path : 'motivomov',                                                                                                                                                                                                                                      
						formatter : '.whitespace2Char'                                                                                                                                                                                                                           
					}" additionalText="{                                                                                                                                                                                                                                      
						path : 'Grtxt',                                                                                                                                                                                                                                          
						formatter : '.whitespace2Char'                                                                                                                                                                                                                           
					}"/>                                                                                                                                                                                                                                                      
                </suggestionItems>                                                                                                                                                                                                                             
            </Input>                                                                                                                                                                                                                                           
        </table:template>                                                                                                                                                                                                                                      
        <table:customData>                                                                                                                                                                                                                                     
            <core:CustomData key="p13nData" value="\{&quot;columnIndex&quot;: &quot;5&quot;, &quot;leadingProperty&quot;: &quot;motivomov&quot;, &quot;columnKey&quot;: &quot;motivomov&quot;}"/>                                                              
        </table:customData>                                                                                                                                                                                                                                    
    </table:Column>                                                                                                                                                                                                                                            
    <table:Column id="_IDGenColumn3" width="10rem">                                                                                                                                                                                                            
        <Label id="_IDGenLabel14" text="Tipo do Movimento"/>                                                                                                                                                                                                   
        <table:template>                                                                                                                                                                                                                                       
            <Input id="tipomov"&nbsp;                                                                                                                                                                                                                          
                valueHelpRequest=".valueHelpTipo"&nbsp;                                                                                                                                                                                                        
                showSuggestion="true"&nbsp;                                                                                                                                                                                                                    
                showValueHelp="true"&nbsp;                                                                                                                                                                                                                     
                valueStateText="Tipo de movimento não existe"&nbsp;                                                                                                                                                                                            
                value="{path : 'tipomov'}"&nbsp;                                                                                                                                                                                                               
                submit=".onSubmitTipo"&nbsp;                                                                                                                                                                                                                   
                maxLength="4"                                                                                                                                                                                                                                  
                suggestionItems="{                                                                                                                                                                                                                             
				path: '/ZI_MM_VH_BWART',                                                                                                                                                                                                                                   
				length: 10                                                                                                                                                                                                                                                 
			}">                                                                                                                                                                                                                                                         
                <suggestionItems>                                                                                                                                                                                                                              
                    <core:ListItem id="_IDGenListItem4" key="{tipomov}" text="{                                                                                                                                                                                
						path : 'tipomov',                                                                                                                                                                                                                                        
						formatter : '.whitespace2Char'                                                                                                                                                                                                                           
					}"/>                                                                                                                                                                                                                                                      
                </suggestionItems>                                                                                                                                                                                                                             
            </Input>                                                                                                                                                                                                                                           
        </table:template>                                                                                                                                                                                                                                      
        <table:customData>                                                                                                                                                                                                                                     
            <core:CustomData key="p13nData" value="\{&quot;columnIndex&quot;: &quot;6&quot;, &quot;leadingProperty&quot;: &quot;tipomov&quot;, &quot;columnKey&quot;: &quot;tipomov&quot;}"/>                                                                  
        </table:customData>                                                                                                                                                                                                                                    
    </table:Column>                                                                                                                                                                                                                                            
</core:FragmentDefinition>                                                                                                                                                                                                                                     