<core:FragmentDefinition                                                                                                                                                                                                                                       
	xmlns="sap.ui.comp.valuehelpdialog"                                                                                                                                                                                                                           
	xmlns:m="sap.m"                                                                                                                                                                                                                                               
	xmlns:fb="sap.ui.comp.filterbar"                                                                                                                                                                                                                              
	xmlns:core="sap.ui.core"                                                                                                                                                                                                                                      
	xmlns:f="sap.ui.layout.form"                                                                                                                                                                                                                                  
	xmlns:mvc="sap.ui.core.mvc">                                                                                                                                                                                                                                  
	 <ValueHelpDialog id="_IDGenValueHelpDialog2"                                                                                                                                                                                                                 
		title="{i18n>Motivo}"                                                                                                                                                                                                                                        
		ok=".onValueHelpDialogOk"                                                                                                                                                                                                                                    
		cancel=".onValueHelpDialogCancel"                                                                                                                                                                                                                            
		supportMultiselect="false"                                                                                                                                                                                                                                   
		key="motivomov"                                                                                                                                                                                                                                              
		descriptionKey="Grtxt" >                                                                                                                                                                                                                                     
		<filterBar>                                                                                                                                                                                                                                                  
			<fb:FilterBar id="filterBarRegion" advancedMode="true" search=".onFilterBarSearch" isRunningInValueHelpDialog="true">                                                                                                                                       
			    <fb:filterGroupItems>                                                                                                                                                                                                                                   
					<fb:FilterGroupItem id="filterMotivo"                                                                                                                                                                                                                     
						groupName="__$INTERNAL$"                                                                                                                                                                                                                                 
						name="motivomov"                                                                                                                                                                                                                                         
						label="Motivo"                                                                                                                                                                                                                                           
						visibleInFilterBar="true">                                                                                                                                                                                                                               
						<fb:control>                                                                                                                                                                                                                                             
							<m:Input id="inMotivo" name="{i18n>Motivo}"                                                                                                                                                                                                             
							showSuggestion="true"                                                                                                                                                                                                                                   
                            suggestionItemSelected=".onInputSuggestionItemSelected"                                                                                                                                                                            
							suggestionItems="{                                                                                                                                                                                                                                      
							   path: '/MotivoVH',                                                                                                                                                                                                                                   
							   sorter: { path: 'motivomov'}                                                                                                                                                                                                                         
						    }">                                                                                                                                                                                                                                                  
								<core:Item id="itemMotivo" key="{motivomov}" text="{                                                                                                                                                                                                   
									path: 'motivomov',                                                                                                                                                                                                                                    
									formatter : ''                                                                                                                                                                                                                                        
								}"/>                                                                                                                                                                                                                                                   
				  			</m:Input>                                                                                                                                                                                                                                            
						</fb:control>                                                                                                                                                                                                                                            
					</fb:FilterGroupItem>                                                                                                                                                                                                                                     
					<fb:FilterGroupItem id="FilterGrtxt"                                                                                                                                                                                                                      
						groupName="__$INTERNAL$"                                                                                                                                                                                                                                 
						name="Grtxt"                                                                                                                                                                                                                                             
						label="Descrição"                                                                                                                                                                                                                                        
						visibleInFilterBar="true">                                                                                                                                                                                                                               
						<fb:control>                                                                                                                                                                                                                                             
							<m:Input id="inGrtxt" name="{i18n>MotivoDesc}"                                                                                                                                                                                                          
								showSuggestion="true"                                                                                                                                                                                                                                  
                                suggestionItemSelected=".onInputSuggestionItemSelected"                                                                                                                                                                        
								suggestionItems="{                                                                                                                                                                                                                                     
									path: '/MotivoVH',                                                                                                                                                                                                                                    
									sorter: { path: 'Grtxt' }                                                                                                                                                                                                                             
							    }">                                                                                                                                                                                                                                                 
								<core:Item id="itemGrtxt" key="{motivomov}" text="{Grtxt}"/>                                                                                                                                                                                           
							</m:Input>                                                                                                                                                                                                                                              
						</fb:control>                                                                                                                                                                                                                                            
				    </fb:FilterGroupItem>                                                                                                                                                                                                                                  
				</fb:filterGroupItems>&nbsp;                                                                                                                                                                                                                               
			</fb:FilterBar>                                                                                                                                                                                                                                             
		</filterBar>                                                                                                                                                                                                                                                 
	</ValueHelpDialog>                                                                                                                                                                                                                                            
</core:FragmentDefinition>                                                                                                                                                                                                                                     