sap.ui.define([                                                                                                                                                                                                                                                
    "sap/m/MessageToast"                                                                                                                                                                                                                                       
], function(MessageToast) {                                                                                                                                                                                                                                    
    'use strict';                                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                               
    return {                                                                                                                                                                                                                                                   
        Executar: function(oEvent) {                                                                                                                                                                                                                           
            let oFilter = this.getView().byId("listReportFilter").getFilters();                                                                                                                                                                                
                                                                                                                                                                                                                                                               
            // get the Model reference                                                                                                                                                                                                                         
            var oModel = this.getView().getModel();                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
            let oCtx = this.templateBaseExtension.getExtensionAPI().getSelectedContexts();                                                                                                                                                                     
            return new Promise((resolve, reject) => {                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
                // Perform Read operation and pass billingdoc as parameter to URL                                                                                                                                                                              
                oModel.read("/ZC_MM_WMS_EXEC_PROCESSO_CE", {                                                                                                                                                                                                   
                    filters: oFilter,                                                                                                                                                                                                                          
                    success: function (oData, oResponse) {                                                                                                                                                                                                     
                        resolve(oData);                                                                                                                                                                                                                        
                        MessageToast.show(oData.results[0].message);                                                                                                                                                                                           
                    },                                                                                                                                                                                                                                         
                    error: function (oError) {                                                                                                                                                                                                                 
                        reject(oError);                                                                                                                                                                                                                        
                    }                                                                                                                                                                                                                                          
                });                                                                                                                                                                                                                                            
            })                                                                                                                                                                                                                                                 
        }                                                                                                                                                                                                                                                      
    };                                                                                                                                                                                                                                                         
});                                                                                                                                                                                                                                                            